<instance id="commcaresession" src="jr://instance/session"/>
			
			<instance id="casedb" src="jr://instance/casedb"/>
			
	    	<setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/>
	    	<setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/>
	    	<setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/>
	    	<setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/>
	    	<setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/>
	    	<setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/>
    		<setvalue event="xforms-ready" ref="/data/case/@case_id" value="instance('commcaresession')/session/data/case_id"/>
    		
			<setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/>    	
			
			<bind nodeset="/data/meta/timeStart" type="dateTime"/>
     		<bind nodeset="/data/meta/timeEnd" type="dateTime"/>	    				
			<bind nodeset="/data/case/@date_modified" type="xsd:dateTime" calculate="/data/meta/timeEnd"/>
			<bind nodeset="/data/case/@user_id" calculate="/data/meta/userID"/>
			
			<bind nodeset="/data/take_weight" required="true()" />
			<bind nodeset="/data/child_weight" type="xsd:double" constraint=". &gt;= 0 and . &lt;= 30" jr:constraintMsg="jr:itext('child_weight-constraintMsg')" relevant="/data/take_weight = 'yes'" required="true()" />
			<bind nodeset="/data/show_grade" relevant="/data/take_weight = 'yes'"/>
			<bind nodeset="/data/display_calc_grade" relevant="/data/take_weight = 'yes'" />
			<bind nodeset="/data/requires_attention" relevant="/data/change_from_normal = 'yes' or /data/change_from_muw = 'yes' or /data/change_from_suw = 'yes'" />
			<bind nodeset="/data/success" />
			<bind nodeset="/data/calc_grade" calculate="if(count(instance('chart')/zscore/gender[@id=/data/child_gender]/age[@id = /data/child_age]/weight[@low &lt;= /data/child_weight][@high = 'none' or @high &gt; /data/child_weight]) = 1, instance('chart')/zscore/gender[@id=/data/child_gender]/age[@id = /data/child_age]/weight[@low &lt;= /data/child_weight][@high = 'none' or @high &gt; /data/child_weight], 'invalid entry')" />
			<bind nodeset="/data/calc_growth" calculate="if((/data/calc_grade=1 or /data/calc_grade=0 or /data/calc_grade=-1 or /data/calc_grade=-2),'growth_normal',if(/data/calc_grade=-3,'moderately_underweight',if(/data/calc_grade=-4, 'severely_underweight', if(/data/calc_grade=2, 'obese', if(/data/calc_grade=3,'overweight','invalid')))))" />
			<bind nodeset="/data/child_gender" calculate="if(/data/gender='male', 2, 1)" />
			<bind nodeset="/data/child_age" calculate="int((today() - /data/dob) div 30)" />
			<bind nodeset="/data/dob" calculate="date(instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/dob)" />
			<bind nodeset="/data/gender" calculate="instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/gender" />
			<bind nodeset="/data/case/update/current_growth" calculate="if(/data/child_weight != '', /data/calc_growth, '')" />
			<bind nodeset="/data/case/update/last_growth_1" calculate="instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/current_growth" />
			<bind nodeset="/data/case/update/last_growth_2" calculate="instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/last_growth_1" />
			<bind nodeset="/data/case/update/last_growth_3" calculate="instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/last_growth_2" />
			<bind nodeset="/data/case/update/current_weight_date" calculate="if(/data/child_weight != '', today(), '')" />
			<bind nodeset="/data/case/update/last_weight_date" calculate="if(instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/current_weight_date = '' and instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/baby_weight != '', instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/add, instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/current_weight_date)" />
			<bind nodeset="/data/case/update/current_weight" calculate="if(/data/child_weight != '', /data/child_weight, '')" />
			<bind nodeset="/data/case/update/last_weight" calculate="if(instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/current_weight = '' and instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/baby_weight != '', instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/baby_weight, instance('casedb')/casedb/case[@case_id=/data/case/@case_id]/current_weight)" />
			<bind nodeset="/data/change_from_normal" calculate="if(/data/case/update/last_growth_1 != '' and /data/case/update/last_growth_1 = 'growth_normal' and (/data/case/update/current_growth = 'moderately_underweight' or /data/case/update/current_growth = 'severely_underweight'), 'yes', 'no')" />
			<bind nodeset="/data/change_from_muw" calculate="if(/data/case/update/last_growth_1 != '' and /data/case/update/last_growth_1 = 'moderately_underweight' and (/data/case/update/current_growth = 'moderately_underweight' or /data/case/update/current_growth = 'severely_underweight'), 'yes', 'no')" />
			<bind nodeset="/data/change_from_suw" calculate="if(/data/case/update/last_growth_1 != '' and /data/case/update/last_growth_1 = 'severely_underweight' and /data/case/update/current_growth = 'severely_underweight', 'yes', 'no')" />
